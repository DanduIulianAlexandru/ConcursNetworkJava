<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="concurs.clientfx.LoginController">
   <children>
      <Label layoutX="203.0" layoutY="36.0" prefHeight="50.0" prefWidth="194.0" text="Login admin">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Label layoutX="156.0" layoutY="161.0" prefHeight="18.0" prefWidth="77.0" text="Username:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="156.0" layoutY="215.0" prefHeight="22.0" prefWidth="78.0" text="Password:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="264.0" layoutY="159.0" prefHeight="26.0" prefWidth="180.0" />
      <PasswordField fx:id="passwordField" layoutX="265.0" layoutY="214.0" prefHeight="26.0" prefWidth="180.0" />
      <Button fx:id="loginButton" layoutX="261.0" layoutY="295.0" mnemonicParsing="false" onAction="#onLoginButtonClicked" prefHeight="26.0" prefWidth="78.0" text="Log in" />
      <Button fx:id="exitButton" layoutX="498.0" layoutY="346.0" mnemonicParsing="false" onAction="#onExitButtonClicked" prefHeight="26.0" prefWidth="78.0" text="Exit" />
      <Label fx:id="errorLabel" layoutX="156.0" layoutY="255.0" text="*username or password incorrect" textFill="RED" />
   </children>
</AnchorPane>
